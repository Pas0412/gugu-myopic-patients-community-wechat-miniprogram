<survey-question-wrapper vue-id="6b55cdb1-1" question="{{question}}" showTitle="{{showTitle}}" showDescription="{{showDescription}}" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{question.rows}}" wx:for-item="row" wx:for-index="rowIndex" wx:key="rowIndex"><view class="container"><text class="question-title-text">{{row.text}}</text><view class="inner-container"><block wx:for="{{question.columns}}" wx:for-item="column" wx:for-index="columnIndex" wx:key="columnIndex"><view class="item"><block wx:if="{{column.cellType==='dropdown'}}"><view class="choice"><text class="question-text">{{column.title}}</text><picker class="picker" value="{{values[row.value][column.name]}}" range="{{ranges[columnIndex]}}" data-event-opts="{{[['change',[['change',['$event',rowIndex,'$0',columnIndex,'$1'],[[['question.rows','',rowIndex,'value']],[['question.columns','',columnIndex,'name']]]]]]]}}" bindchange="__e"><view class="uni-input">{{ranges[columnIndex][values[row.value][column.name]]}}</view></picker><image class="image-arrow" src="{{arrowIcon}}"></image></view></block><block wx:if="{{column.cellType==='text'||column.cellType==='number'}}"><input class="input" placeholder="{{column.placeHolder||'请输入'+column.title}}" type="{{question.inputType==='number'?'digit':question.inputType}}" data-event-opts="{{[['blur',[['blur',['$event',rowIndex,'$0',columnIndex,'$1'],[[['question.rows','',rowIndex,'value']],[['question.columns','',columnIndex,'name']]]]]]]}}" value="{{values[row.value][column.name]}}" bindblur="__e"/></block><block wx:if="{{columnIndex<question.columns.length-1}}"><view class="spliter"></view></block></view></block></view><block wx:if="{{rowIndex<question.rows.length-1}}"><view class="spliter"></view></block><block wx:if="{{rowIndex<question.rows.length-1}}"><view class="block"></view></block></view></block></survey-question-wrapper>